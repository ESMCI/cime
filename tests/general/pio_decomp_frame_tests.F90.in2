                              (start(2) - 1 + j - 1) * dims(1) + i
          exp_val4d(i,j,k,f) = exp_val4d(i,j,k,f)+(f - 1) * (dims(1) * dims(2) * dims(3))
        end do
      end do
    end do
  end do
  allocate(compdof(nrows * ncols * nhgts))
