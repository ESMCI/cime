        compdof(tmp_idx) = exp_val4d(i,j,k,1)
      end do
    end do
  end do
   ! pio_decomp_frame_tests.F90.in:326
  allocate(rbuf3d(nrows, ncols, nhgts))
  rbuf3d = 0
  ! Expected val for 3d var
  allocate(exp_val3d(nrows, ncols, nhgts))
  do k=1,nhgts
    do j=1,ncols
