        compdof(tmp_idx) =  (start(3) - 1 + k - 1) * (dims(1) * dims(2)) +&
                            (start(2) - 1 + j - 1) * dims(1) + i
        exp_val(i,j,k) = compdof(tmp_idx)
      end do
    end do
  end do
   ! pio_decomp_tests_3d.F90.in:156
  call PIO_initdecomp(pio_tf_iosystem_, PIO_double, dims, compdof, rd_iodesc)
