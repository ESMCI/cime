<?xml version="1.0"?>
<machine MACH="sierra">
  <module_system type="dotkit">
    <init_path lang="csh">/usr/global/tools/dotkit/init.csh</init_path>
    <init_path lang="sh">/usr/global/tools/dotkit/init.sh</init_path>
    <cmd_path lang="csh"/>
    <cmd_path lang="sh"/>
    <modules>
      <command name="use -q">pgi-11.1</command>
      <command name="use -q">mvapich2-pgi-1.7</command>
      <command name="use -q">netcdf-pgi-4.1.3</command>
    </modules>
  </module_system>
  <environment_variables>
    <env name="NETCDF">/usr/local/tools/netcdf-pgi-4.1.3/</env>
  </environment_variables>
</machine>
